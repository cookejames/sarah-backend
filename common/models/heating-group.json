{
  "name": "heatingGroup",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "http": {
    "path": "heating/group"
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "index": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "isEnabled": {
      "type": "boolean"
    }
  },
  "validations": [],
  "relations": {
    "schedules": {
      "type": "embedsMany",
      "model": "heatingSchedule",
      "foreignKey": "groupId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": []
}
